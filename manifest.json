{
    "name": "UwUify",
    "description": "Will make all pages twanswated to UwU wanguage",
    "version": "1.1.2",
    "manifest_version": 3,

    "permissions": [
        "activeTab"
    ],

    "action": {
        "browser_style": true,
        "default_popup": "options.html",
        "default_icon": {
          "128": "icon.png"
        }
      },

      "browser_specific_settings": {
        "gecko": {
          "id": "uwuifier@thenorwegian.dev"
        }
      }

  }
